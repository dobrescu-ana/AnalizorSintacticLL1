<Page x:Class="Interface.ShowGrammarPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Interface"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ShowGrammarPage">

    <Grid>
        <StackPanel>
            <TextBlock Text="Simbol start" Margin="0 10 0 0" Foreground="White" FontSize="20" />
            <TextBlock x:Name="simbolstartTB" Foreground="White"  FontSize="20"/>

            <TextBlock Text="Multime Neterminale"  Margin="0 10 0 0" Foreground="White" FontSize="20" />
            <TextBlock x:Name="neterminaleTB" Foreground="White" FontSize="20"  />

            <TextBlock Text="Multime Terminale" Margin="0 10 0 0" Foreground="White" FontSize="20"/>
            <TextBlock x:Name= "terminaleTB" Foreground="White" FontSize="20" />

            <TextBlock Text="Numarul regulilor de productie" Margin="0 10 0 0" Foreground="White" FontSize="20"/>
            <TextBlock x:Name="nrReguliTB" Foreground="White" FontSize="20" />

            <TextBlock Text="Reguli de productie" Margin="0 10 0 0" Foreground="White" FontSize="20"/>
            <TextBlock x:Name="reguliTB" Foreground="White" FontSize="20" />
        </StackPanel>
    </Grid>
</Page>
